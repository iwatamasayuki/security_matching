<%= form_with model: @guard, local: true do |f| %>
  <div class="field">
    <%= f.label :company, "所属会社" %><br />
    <%= f.text_field :company%>
  </div>
  <div class="field">
    <%= f.label :image, "本人写真" %><br />
    <%= f.file_field :image%>
  </div>
  <div class="field">
    <%= f.label :guard_name, "警備員名" %><br />
  <%= f.text_field :guard_name%>
  </div>
  <div class="field">
    <%= f.label :name_kana, "フリガナ" %><br />
    <%= f.text_field :name_kana%>
  </div>
  <div class="field">
    <%= f.label :gender, "性別" %><br />
    <%= f.text_field :gender%>
  </div>
  <div class="field">
    <%= f.label :age, "年齢" %><br />
    <%= f.text_field :age%>
  </div>
  <div class="field">
    <%= f.label :experience, "経験年数" %><br />
    <%= f.text_field :experience%>
  </div>
  <div class="field">
    <%= f.label :qualification, "保有資格" %><br />
    <%= f.text_field :qualification%>
  </div>
  <%= f.submit "登録" %>
<% end %>